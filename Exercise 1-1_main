void setup(){
   student suho, jinhun, hyunjung ;
   suho = new student();
   jinhun = new student();
   hyunjung = new student();
   suho.test();
   jinhun.test();
   hyunjung.test();
   
}

class student {
  student(){w = 60; h = 180 ; grade = (int) random(60,101) ;}
  float w,h,r;
  int grade;
  void test() {
      if (grade > 80) println("Good!!");
      else {println("Study!!");}  
  }
}
